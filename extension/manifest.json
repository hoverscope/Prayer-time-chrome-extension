{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A base template for a Chrome extension.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
      
        "128": "icon.png"
      }
    },
    
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting","notifications", "alarms"]
  }
  